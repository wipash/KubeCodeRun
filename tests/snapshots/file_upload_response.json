{
  "_description": "Expected /files/upload response format",
  "_version": "1.0.0",
  "_librechat_compatible": true,

  "request": {
    "method": "POST",
    "endpoint": "/files/upload",
    "content_type": "multipart/form-data",
    "fields": {
      "files": "binary file(s)",
      "entity_id": "optional session/entity identifier"
    }
  },

  "response": {
    "files": [
      {
        "id": "string (unique file identifier)",
        "name": "string (original filename)",
        "size": "integer (file size in bytes)",
        "session_id": "string (session the file belongs to)"
      }
    ]
  },

  "response_example": {
    "files": [
      {
        "id": "file-svc-abc123def456",
        "name": "data.csv",
        "size": 1024,
        "session_id": "entity-xyz789"
      }
    ]
  },

  "notes": [
    "session_id matches entity_id if provided",
    "session_id is 'temp-session' if no entity_id",
    "Multiple files can be uploaded in single request",
    "File ID format is generated by the service"
  ]
}
